native nohold _printf();

input void SDL_QUIT;

class BlinkA with
    var int dt;
    var int id;
do
    every (this.dt)ms do
        _printf("[%d] toggle\n", this.id);
    end
end

var BlinkA* ptr;

do
    var BlinkA b1 with
        this.id = 1;
        this.dt = 5000;
    end;

    ptr = &b1;

    var BlinkA b2 with
        this.id = 2;
        this.dt = 2500;
    end;

    await 8s;
end

_printf("FIM: %p\n", ptr);

await SDL_QUIT;
return 0;
