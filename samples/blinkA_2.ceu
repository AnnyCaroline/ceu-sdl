input void SDL_QUIT;

class BlinkA with
    var int dt;
    var int id;
do
    every (this.dt)ms do
        _printf("[%d] toggle\n", this.id);
    end
end

do
    var BlinkA b1 with
        _.id = 1;
        _.dt = 5000;
    end;

    var BlinkA b2 with
        _.id = 2;
        _.dt = 2500;
    end;

    await 8s;
end

_printf("FIM\n");

await SDL_QUIT;
escape 0;
