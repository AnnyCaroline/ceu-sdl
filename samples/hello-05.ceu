class HelloWorld with
    var int id;
do
    every 1s do
        _printf("[%d] Hello world!\n", this.id);
    end
end

watching 10s do
    par/or do
        await 5s;
    with
        do
            pool HelloWorld[] hs;
            var int i = 1;
            every 1s do
                spawn HelloWorld in hs with
                    this.id = i;
                end;
                i = i + 1;
            end
        end
    end
end

escape 0;
